<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="emp_pane1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Employee.employeeOptions">
   <children>
      <Label alignment="CENTER" layoutX="28.0" layoutY="38.0" prefHeight="48.0" prefWidth="192.0" text="Delete Employee" textFill="#fff7f7">
         <font>
            <Font name="System Bold Italic" size="23.0" />
         </font>
      </Label>
      <Button fx:id="del_emp" layoutX="35.0" layoutY="238.0" mnemonicParsing="false" onAction="#deleteEmployee" prefHeight="48.0" prefWidth="152.0" text="Delete Employee">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <TextField fx:id="employeeID" layoutX="256.0" layoutY="177.0" prefHeight="31.0" prefWidth="223.0" promptText="Enter Empployee ID" />
      <Label alignment="CENTER" layoutX="9.0" layoutY="168.0" prefHeight="48.0" prefWidth="247.0" text="Enter Employee ID" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="21.0" />
         </font>
      </Label>
      <Button fx:id="Main2" layoutX="300.0" layoutY="238.0" mnemonicParsing="false" onAction="#mainpage1" prefHeight="48.0" prefWidth="152.0" text="Go Back">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
