<AnchorPane fx:id="con_pane1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Contract.contractOptions">
   <children>
      <Label alignment="CENTER" layoutX="14.0" layoutY="158.0" prefHeight="48.0" prefWidth="232.0" text="Enter Contract ID" textFill="#fcfcfc">
         <font>
            <Font name="System Bold Italic" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="contractID" layoutX="246.0" layoutY="167.0" prefHeight="31.0" prefWidth="223.0" promptText="Enter Contract ID" />
      <Button fx:id="delcon" layoutX="83.0" layoutY="231.0" mnemonicParsing="false" onAction="#deleteContract" prefHeight="48.0" prefWidth="152.0" text="Delete Contract">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Button>
      <Label layoutX="18.0" layoutY="28.0" prefHeight="48.0" prefWidth="198.0" text="Delete Contract" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <Button fx:id="Main" layoutX="280.0" layoutY="231.0" mnemonicParsing="false" onAction="#mainpage1" prefHeight="48.0" prefWidth="152.0" text="Go Back">
